---
import Page from "./Base.astro";

interface Props {
  frontmatter: {
    title: string;
    direct_link: string;
  };
}

const { frontmatter } = Astro.props;
---

<Page title={frontmatter.title}>
  <header slot="header">
    <h1>
      <a href="/cybersec">{frontmatter.title}</a>
    </h1>
  </header>
  <a href={frontmatter.direct_link}>{frontmatter.direct_link}</a>
  <slot />
</Page>
