---
import Page from "../../templates/page.astro";
import ArtWithClickthrough from "../../components/art/art_with_clickthrough.svelte";
import ArtWithDesc from "../../components/art/art_with_desc.astro";
import TwitterLink from "../../components/twitter_link.svelte";
import InstagramLink from "../../components/instagram_link.svelte";

import i2024_05_bunnysuit from "./images/_2024-05-bunnysuit.png";
import i2023_05_bunnysuit from "./images/_2023-05-bunnysuit.png";
import i2023_02_sidelunge from "./images/_2023-02-sidelunge.png";
import i2023_01_gmang from "./images/_2023-01-gmang.png";
import ss2023_jay_day from "./images/ss/_2023_jay-day.jpg";
